<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" minWidth="120.0" prefHeight="40.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal">
    <children>
        <Label fx:id="camName" text="CamName" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
               AnchorPane.topAnchor="10.0"/>
        <Button fx:id="deleteCell" mnemonicParsing="false" text="x" textAlignment="CENTER"
                AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <font>
                <Font size="10.0"/>
            </font>
        </Button>
        <Button fx:id="editCell" mnemonicParsing="false" text="E" AnchorPane.bottomAnchor="10.0"
                AnchorPane.rightAnchor="35.0" AnchorPane.topAnchor="10.0">
            <font>
                <Font size="10.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
